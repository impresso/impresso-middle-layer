{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Authentication Response",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "accessToken": {
      "type": "string"
    },
    "authentication": {
      "type": "object",
      "properties": {
        "strategy": {
          "type": "string"
        },
        "payload": {
          "type": "object"
        }
      }
    },
    "user": {
      "$ref": "schema/schemas/User.json"
    }
  },
  "required": ["accessToken", "authentication", "payload", "user"]
}
